programa {
  funcao inicio() {
    "comparar_numeros"

    inteiro numero1, numero2

    escreva("Digite o primeiro número: ")
    leia(numero1)
    
    escreva("Digite o segundo número: ")
    leia(numero2)

    se (numero1 > numero2)
        escreva("O primeiro número é maior que o segundo.\n")
    senao
        escreva("O primeiro número não é maior que o segundo.\n")
  

  }
}
